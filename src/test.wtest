import clases.*



describe "Test de las personas" {
	const rebeca = new PersonaBancarizada(nombre = "Rebeca")

 	method initialize() {
 		rebeca.cobrar(1000)
 	}

	test "ahorros de les pibes" {
		pepe.cobrar(2000)
		pepe.gastar(100)
		assert.equals(1000, rebeca.ahorros())
		assert.equals(1900, pepe.ahorros())
	}

	test "ir a comer" {
		rebeca.salirAComer()
		assert.equals(500, rebeca.ahorros())
	}

}

